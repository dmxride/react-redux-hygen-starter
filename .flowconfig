[ignore]

[include]

[untyped]
.*/node_modules/*

[libs]

[lints]

[options]
module.name_mapper='^src\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^components\/\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^containers\/\(.*\)$' -> '<PROJECT_ROOT>/src/containers/\1'
module.name_mapper='^scenes\/\(.*\)$' -> '<PROJECT_ROOT>/src/scenes/\1'
module.name_mapper='^app\/\(.*\)$' -> '<PROJECT_ROOT>/src/app/\1'
module.name_mapper='^assets\/\(.*\)$' -> '<PROJECT_ROOT>/src/assets/\1'
module.file_ext=.js
module.file_ext=.scss
module.name_mapper.extension='scss' -> 'empty/object'

[strict]
